boolean isValid(String s){
  Stack<Character>st=new Stack<>();
  for(char ch: s.toCharArray()){
    if(ch=='(' || ch=='[' || ch=='{'){
      st.push(ch);
    }
    else{
      return false;
    }
  }
  return st.isEmpty();
}

boolean check(char ob,char cb){
  if(ob=='(' && cb==')') return true;
  if(ob=='[' && cb==']') return true;
  if(ob=='{' && cb=='}') return true;
}
